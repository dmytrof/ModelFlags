{
    "name": "dmytrof/model-flags",
    "type": "library",
    "description": "Adds flags support to your model/entity",
    "keywords": ["flag", "entity flags", "model flags"],
    "license": "MIT",
    "version": "v1.1",
    "time": "2023-12-29",
    "authors": [
        {
            "name": "Dmytro Feshchenko",
            "email": "dmytro.feshchenko@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5 || ^10.5",
        "squizlabs/php_codesniffer": "^3.8",
        "vimeo/psalm": "^5.18"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Dmytrof\\ModelFlags\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dmytrof\\ModelFlags\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "tests": "phpunit"
    }
}
